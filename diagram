Table article {
  id int [pk, increment]
  url tinytext [not null, unique]
  title tinytext
  r_date date
  img tinytext
  txt_id int 
  author_id int
  section_id int
}

Table author {
  id int [pk, increment]
  name tinytext [not null, unique]
  title tinytext
}

Table section {
  id int [pk, increment]
  name tinytext [not null, unique]
  url tinytext [not null, unique]
}

Table txt {
  id int [pk, increment]
  summary text
  article_text text
}


Table article_label {
  id int [pk, increment]
  label_id int
  article_id int
}


Table label {
  id int [pk, increment]   
  name tinytext [not null, unique]
  url tinytext [not null, unique]
}

Table article_link {
  id int [pk, increment]
  article_id int 
  link_id int
}

Table link {
  id int [pk, increment]  
  url tinytext 
}


Ref: article.author_id > author.id 
Ref: article.section_id > section.id
Ref: txt.id - article.txt_id

Ref: article_label.article_id > article.id
Ref: article_label.label_id > label.id

Ref: article_link.article_id > article.id
Ref: article_link.link_id > link.id

